{
  "env": {
    "es6": true,
    "browser": true,
    "react-native/react-native": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "comma-dangle": 0,
    "semi": 0,
    "no-underscore-dangle": "off",
    "consistent-return": 0,
    "radix": 0,
    "class-methods-use-this": 0,
    "object-curly-newline": 0,
    "no-shadow": 1,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "no-debugger": "warn",
    "no-undef": 0,
    "quotes": 0,
    "no-restricted-syntax": 0,
    "no-plusplus": 0,
    "no-else-return": 0,
    "max-len": 0,
    "no-param-reassign": 0
  },
  # "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["html", "react", "react-native", "jest"]
}
